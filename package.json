{
  "name": "@onoranzefunebricloud/serverless-bundle",
  "version": "1.4.2",
  "description": "An extension of the serverless-webpack plugin that bundles your Node.js Lambda functions.",
  "main": "index.js",
  "scripts": {
    "test": "jest --no-watchman tests/index.test.js",
    "test-scripts": "./bin/scripts.js test --TEST_MODE tests/scripts/test.js"
  },
  "bin": {
    "serverless-bundle": "./bin/scripts.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/scripts/setupTests.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-bundle.git"
  },
  "author": "Jay V <air@live.ca>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^6.1.0",
    "core-js": "^3.6.5",
    "cross-spawn": "^6.0.5",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-loader": "^2.2.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "jest": "^24.9.0",
    "native-ext-loader": "^2.3.0",
    "node-loader": "^0.6.0",
    "regenerator-runtime": "^0.13.7",
    "serverless-webpack": "^5.3.4",
    "source-map-support": "^0.4.18",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
